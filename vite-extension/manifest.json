{
    "manifest_version": 3,
    "name": "OpenSauced GitHub Extension",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "src/OpenSauced.png"
    },"icons": {
        "128": "src/OpenSauced.png"
    },
    "content_scripts": [
        {
            "matches": ["*://github.com/*"],
            "js": ["src/script.js"],
            "run_at": "document_end"
        }
    ]
}